{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
            ],
        "no-console": "off",
        "semi":"off",
        "linebreak-style":"off",
        "quotes":"error"
    }
}
